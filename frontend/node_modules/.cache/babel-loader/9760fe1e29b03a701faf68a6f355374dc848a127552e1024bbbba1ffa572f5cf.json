 Object\r\n});\r\n\r\nconst { product } = useProduct(props.product);\r\n\r\nfunction useProduct(product) {\r\n  const incrementQuantity = () => {\r\n    product.quantity++;\r\n  };\r\n\r\n  const decrementQuantity = () => {\r\n    if (product.quantity > 1) {\r\n      product.quantity--;\r\n    }\r\n  };\r\n\r\n  const addToCart = () => {\r\n    console.log('Added to cart:', product);\r\n    // Implementa la lógica para agregar al carrito aquí\r\n  };\r\n\r\n  return {\r\n    product,\r\n    incrementQuantity,\r\n    decrementQuantity,\r\n    addToCart\r\n  };\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.product-card {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  height: auto;\r\n  padding: 10px;\r\n  box-shadow: 0 6px 8px rgba(0, 0, 0, 0.7);\r\n  gap: 20px;\r\n}\r\n\r\n.product-details {\r\n  text-align: center;\r\n  margin-bottom: auto; /* Alinea los detalles del producto arriba */\r\n}\r\n\r\n.quantity-controls {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-top: auto; /* Alinea los controles de cantidad abajo */\r\n}\r\n\r\n.quantity-controls button {\r\n  margin: 0 2px;\r\n}\r\n\r\n.btn {\r\n  text-decoration: none;\r\n  color: #fff;\r\n  transition: color 0.3s ease;\r\n  border-radius: 10px;\r\n  padding: 10px 20px;\r\n  box-shadow: 0 6px 8px rgba(0, 0, 0, 0.7);\r\n  transition: color 0.3s ease, box-shadow 0.3s ease;\r\n  background-image: linear-gradient(#ff7437, #f03406);\r\n}\r\n\r\n.quantity-controls span {\r\n  font-weight: bold;\r\n  font-size: 20px;\r\n  margin-right: 10px;\r\n  margin-left: 10px;\r\n}\r\n\r\n\r\n.btn:hover {\r\n  background-color: #f03406;\r\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n</style>\r\n"],"mappings":";;;;;;;;;IAoBA,MAAMA,KAAK,GAAGC,OAEZ;IAEF,MAAM;MAAEC;IAAQ,CAAC,GAAGC,UAAU,CAACH,KAAK,CAACE,OAAO,CAAC;IAE7C,SAASC,UAAUA,CAACD,OAAO,EAAE;MAC3B,MAAME,iBAAiB,GAAGA,CAAA,KAAM;QAC9BF,OAAO,CAACG,QAAQ,EAAE;MACpB,CAAC;MAED,MAAMC,iBAAiB,GAAGA,CAAA,KAAM;QAC9B,IAAIJ,OAAO,CAACG,QAAQ,G