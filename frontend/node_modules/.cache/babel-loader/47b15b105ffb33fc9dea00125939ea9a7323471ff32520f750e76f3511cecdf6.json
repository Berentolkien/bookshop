p>\r\nimport NavBar from '@/components/NavBar.vue';\r\nimport { ref, onMounted } from 'vue';\r\n\r\nconst products = ref([]);\r\n\r\nonMounted(async () => {\r\n  try {\r\n    const response = await fetch('http://127.0.0.1:8000/api/products/');\r\n    const data = await response.json();\r\n    data.forEach(product => {\r\n      product.quantity = 1 ;\r\n    });\r\n    products.value = data;\r\n  } catch (error) {\r\n    console.error('Error fetching products:', error);\r\n  }\r\n});\r\n\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.products-container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n\r\n.products-list {\r\n  list-style: none;\r\n  padding: 10px;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  gap: 20px;\r\n  width: 90%;\r\n}\r\n\r\n.product-item {\r\n  width: calc(33.33% - 20px);\r\n  text-align: center;\r\n  padding: 20px;\r\n  box-shadow: 0 6px 8px rgba(0, 0, 0, 0.7);\r\n}\r\n\r\n.product-item img {\r\n  width: 40%;\r\n  height: auto;\r\n  border-radius: 10px;\r\n}\r\n\r\n.product-item h3 {\r\n  margin-top: 10px;\r\n}\r\n\r\n.product-item p {\r\n  margin-top: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AAuBA,OAAOA,MAAM,MAAM,yBAAyB;AAC5C,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;;;;;;;IAEpC,MAAMC,QAAQ,GAAGF,GAAG,CAAC,EAAE,CAAC;IAExBC,SAAS,CAAC,YAAY;MACpB,IAAI;QACF,MAAME,QAAQ,GAAG,MAAMC,KAAK,CAAC,qCAAqC,CAAC;QACnE,MAAMC,IAAI,GAAG,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;QAClCD,IAAI,CAACE,OAAO,CAACC,OAAO,IAAI;UACtBA,OAAO,CAACC,QAAQ,GAAG,CAAC;QACtB,CAAC,CAAC;QACFP,QAAQ,CAACQ,KAAK,GAAGL,IAAI;MACvB,CAAC,CAAC,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}